.SearchView {
  flex-grow: 1;
  border-width: 1px;
  border-color: var(--appui-base-400);
  border-radius: 8px;
  padding-bottom: 12px;
}

.SearchView-Label__Title {
  padding: 4px;
  font-size: 14px;
  -unity-text-align: middle-center;
  -unity-font-style: bold;
}

.SearchView-NavigateButton {
  height: 24px;
  margin: 0px;
  padding: 0px;
  border-width: 0;
  border-radius: 0px;
  justify-content: center;
  -unity-font-style: bold;
  background-color: transparent;
}

.SearchView-NavigateButton:disabled {
  opacity: 1.0;
}

.SearchView-Icon__Return {
  height: 16px;
  width: 16px;
  visibility: hidden;
}

.UnityThemeDark .SearchView-Icon__Return {
  background-image: url("/Packages/com.unity.behavior/Tools/SearchWindow/Assets/Icons/d_tab_prev.png"); 
}

.UnityThemeLight .SearchView-Icon__Return {
  background-image: url("/Packages/com.unity.behavior/Tools/SearchWindow/Assets/Icons/d_tab_prev.png");
}

.SearchView-ListView__Results {
  flex-grow: 1;
  padding: 0px;
  margin: 3px;
  -unity-text-align: middle-left;
  
}

#ReturnButton {
  flex-direction: column;
  height: 40px;
  padding-left: 12px;
  padding-right: 12px;
}

#ReturnButton > Label {
  position: absolute;
  align-self: center;
}

SearchBar {
  margin-left: 12px;
  margin-right: 12px;
  margin-bottom: 8px;
  width: auto;
}

#SearchTabs {
  margin-left: 12px;
  margin-right: 12px;
  align-items: flex-start;
  max-height: 28px;
  padding-bottom: 8px;
  display: none;
  flex-shrink: 0;
}