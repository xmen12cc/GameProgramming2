<ui:UXML xmlns:ui="UnityEngine.UIElements"
         xsi="http://www.w3.org/2001/XMLSchema-instance" engine="UnityEngine.UIElements" editor="UnityEditor.UIElements"
         noNamespaceSchemaLocation="../../../../UIElementsSchema/UIElements.xsd" editor-extension-mode="False"
         xmlns:appui="Unity.AppUI.UI">
    <ui:VisualElement name ="LeftContainer">
        <appui:ActionGroup name="AssetActionsGroup">
            <appui:ActionButton name="OpenAssetButton" label="Open" icon="folder"/>
        </appui:ActionGroup>
    </ui:VisualElement>
    <ui:VisualElement name="AssetTitleContainer" picking-mode="Ignore">
        <ui:VisualElement name="AssetTitleElementContainer" picking-mode="Ignore">
            <appui:Icon name="AssetIcon"/>
            <appui:Text name="AssetTitleElement" picking-mode="Ignore"/>
        </ui:VisualElement>
    </ui:VisualElement>
    <ui:VisualElement name="RightContainer">
    </ui:VisualElement>
</ui:UXML>
